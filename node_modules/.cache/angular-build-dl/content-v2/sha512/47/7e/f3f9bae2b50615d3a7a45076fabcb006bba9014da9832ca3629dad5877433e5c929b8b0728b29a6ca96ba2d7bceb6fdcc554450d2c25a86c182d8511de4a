X\nasync showPickerMax() {\n  const options: PickerOptions = {\n    buttons: [\n      {\n        text: 'Cancel',\n        role: 'cancel'\n      },\n      {\n        text: 'Ok',\n        handler: (value: any) => {\n        }\n      }\n    ],\n    columns: [{\n      name: 'max',\n      options: this.getColumnOptions()\n    }]\n  };\n  const picker = await this.pickerController.create(options);\n  picker.present();\n }\n\n // CHOOSE SEX\nasync showPickerSex() {\n  const options: PickerOptions = {\n    buttons: [\n      {\n        text: 'Cancel',\n        role: 'cancel'\n      },\n      {\n        text: 'Ok',\n        handler: (value: any) => {\n        }\n      }\n    ],\n    columns: [{\n      name: 'Sex',\n      options: [{text: 'Man', value: 'Woman'}, {text: 'Man', value: 'Woman'}]\n    }]\n  };\n  const picker = await this.pickerController.create(options);\n  picker.present();\n }\n\ngetColumnOptions() {\n  // tslint:disable-next-line:prefer-const\n  let options = [];\n  for (let i = 18; i < 90; i++) {\n    options.push({text: i, value: i });\n  }\n  return options;\n}\n}\n"]}